{
    "env": {
      "browser": true,
      "es2021": true,
      "node": true
    },
    "extends": [
      "plugin:vue/essential"
      // Add other extends if needed
    ],
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "plugins": [
      "vue"
      // Add other plugins if needed
    ],
    "rules": {
      "vue/multi-word-component-names": "off"
      // Add other rules if needed
    }
  }
  